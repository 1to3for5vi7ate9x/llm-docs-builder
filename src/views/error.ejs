<div class="container">
  <div class="error-page">
    <h1>Error</h1>
    <p>An error occurred while processing your request.</p>
    <% if (process.env.NODE_ENV === 'development' && error) { %>
      <pre><%= error.stack %></pre>
    <% } %>
    <a href="/" class="btn">Go to Home</a>
  </div>
</div>